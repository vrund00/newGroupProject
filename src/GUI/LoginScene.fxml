<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.LoginController">
   <children>
      <Button fx:id="loginButton" layoutX="127.0" layoutY="144.0" mnemonicParsing="false" onAction="#validateLogin" text="Login" />
      <TextField fx:id="usernameID" layoutX="59.0" layoutY="61.0" promptText="Username">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordID" layoutX="59.0" layoutY="99.0" promptText="Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="forgotPassB" layoutX="96.0" layoutY="182.0" mnemonicParsing="false" onAction="#forgot" text="Forgot Password" />
      <Button fx:id="newUser" layoutX="115.0" layoutY="226.0" mnemonicParsing="false" onAction="#newUser" text="New User" />
      <Label fx:id="status" layoutX="127.0" layoutY="35.0" text="Label" />
   </children>
</AnchorPane>
